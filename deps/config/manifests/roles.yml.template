- kind: Role
  name: anonymous
  specs:
    permission:
      - proxy:nodes
- kind: Role
  name: guest
  specs:
    permission:
      - read:*
- kind: Role
  name: signer
  specs:
    permission:
      - read:*
      - sign:keys
      - sign:eth1accounts
- kind: Role
  name: admin
  specs:
    permission:
      - *:*
