- kind: role
  specs:
    name: anonymous
    permissions:
      - "proxy:nodes"

- kind: role
  specs:
    name: guest
    permissions:
      - "read:*"

- kind: role
  specs:
    name: signer
    permission:
      - "read:*"
      - "sign:keys"
      - "sign:ethereum"

- kind: role
  specs:
    name: admin
    permissions:
      - "*:*"
